{% extends 'AppBundle:Layouts:application.html.twig' %}
{% set translationDomain = "report-odr" %}
{% trans_default_domain translationDomain %}

{% block pageTitle %}{{ 'page.htmlTitle' | trans }}{% endblock %}
{% block bodyClasses %}home-odr{% endblock %}
{% block pageContent %}
    <header class="page-header">
        <h1 class="heading-xlarge">{{ 'page.title' | trans }}
            <p class="heading" style="color: #c0c0c0">{{ client.firstname }} {{ client.lastname }}</p>
        </h1>

    </header>
    {# temporary styles for prototype. do NOT merge those to master #}
    <style type="text/css">
        .home-odr h2.title {
            border-bottom: 1px solid grey;
            margin-bottom: 20px;
            font-size: 1.5em;
            font-weight: bold;
        }
        .home-odr h2 img {
            vertical-align: middle
        }
        .home-odr .box {
            border: 1px solid grey;
            margin-bottom: 20px;
            padding: 10px;
        }
        .home-odr .box img {
            border: 1px solid grey;
            margin-bottom: 20px;
        }
        .home-odr .box p.clientName {
            font-weight: bold !IMPORTANT;
            display: block;
        }
        .home-odr .box p.grey {
            display: block;
            color: grey;
        }
        .rounded {
            font-size: 0.8em;
            border-radius: 5px;
            border: 2px solid #4a4a4a;
            padding: 2px;
            width: auto;
            float: right;
            font-weight:bold;
        }
        .rounded-grey {
            color: grey;
            border-color: grey;
        }
    </style>
    {# end temporary style #}
    <div id="page-body">



        {# ODR #}
        <h2 class="title" >
            <img src="data:image/png;base64," />
            Opening deputyship report</h2>
        <div class="box">
            <div class="rounded rounded-grey">NOT STARTED</div>
            <p class="clientName">{{ client.firstname }} {{ client.lastname }}</p>
            <p class="grey">You only need to complete this report once</p>
            <a href="#" class="button">Start now</a>
        </div>


        {# REPORT #}
        <h2 class="title" >
            <img src="data:image/png;base64," />
            Property and finance
        </h2>

        <div class="box" style="background-color: lightgrey">
            <div class="rounded">NOT AVAILABLE</div>
            <p class="clientName">{{ client.firstname }} {{ client.lastname }}</p>
            <p class="grey">This report will become available once the opening deputy report is complete</p>

        </div>




        {# see _list-reports.html.twig for field rendering #}
        {#{% for report in reports %}#}
            {#{% set linkToReportOverviewPage = path('report_overview', { 'reportId': report.id }) %}#}
            {#<div style="border: 1px solid #c0c0c0; ">#}
                {#<h2>{{ client.firstname }}</h2>#}
                {#<div>{{ report.period }}</div>#}
                {#<div>Due: {{ report.dueDate | date }}</div>#}
                {#<span class="button-wrapper">#}
                    {#<a href="{{ linkToReportOverviewPage }}" class="open-button button behat-link-report-open behat-link-report-{{ report.getPeriod() | behat_namify }}-open">{{ 'reportDataList.startNow' | trans }}</a>#}
                {#</span>#}
                {#<div>#}
                    {#not started#}
                {#</div>#}
            {#</div>#}
        {#{% endfor %}#}

    </div>

{% endblock %}





