{% extends 'AppBundle:Layouts:application.html.twig' %}
{% block pageTitle %}Deputyship Report{% endblock %}
{% block contentClass %}page-report-content{% endblock %}

{% if (showNav is not defined) %}
{%     set showNav = true %}
{% endif %}

{% if (showHelp is not defined) %}
{%     set showHelp = true %}
{% endif %}


{% block pageContent %}
    {% block pageHeader %}
        {% include 'AppBundle:Components/PageHeader:_report-header.html.twig' %}
    {% endblock %}

    <a name="pageBody"></a>

    {% if showNav == true %}
        {% block pageNavigation %}
            {% include 'AppBundle:Components/Navigation:_report-nav.html.twig' with {
                'report': report
            } %}
        {% endblock %}
    {% endif %}

    <div id="page-section-title-container" class="page-section-title-container">
        <h1>{% block title %}{% endblock %}</h1>
        {% include 'AppBundle:Components:_helpline.html.twig' %}
    </div>
    
    <div id="page-body">
        <div id="pagebody-left-content">
           {% block pageBody %}{% endblock %}
        </div>
    </div>

{% endblock %}
