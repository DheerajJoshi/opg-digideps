<div class="financial-categories" id="{{ moneyType }}-transactions">
    {% for categoryId, transactionGroup in report.groupByCategory(transactions) %}
        
        {% include 'AppBundle:Report:Formatted/Accounts/_category.html.twig' with {
        'categoryId': categoryId,
        'transactions': transactionGroup.entries,
        'total': transactionGroup.amountTotal,
        'moneyType': moneyType
        } %}

    {% endfor %}
    
    <div class="transaction total">
        <div class="label">Money {{ type }} total</div>
        <div class="value">&pound; {{ total | number_format(2, '.', ',') }}</div>
    </div>
</div>
