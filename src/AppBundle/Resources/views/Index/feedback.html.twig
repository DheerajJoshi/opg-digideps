{% extends 'AppBundle:Components/Page:page-service.html.twig' %}
{% set translationDomain = "feeback" %}
{% set trans_default_domain = translationDomain %}

{% block pageTitle %}Deputyship service - Your feedback{% endblock %}

{% block title %}Your feedback{% endblock %}



{% block pageHeader %}
    {{ parent() }}
{% endblock %}

{% block pageNavigation %}
{% endblock %}

{% block validationSummary %}
    
{% endblock %}


{% block pageBody %}

    {% set formClass = 'in-page-form feedback-form' %}

    {{ form_start(form, {attr: {novalidate: 'novalidate', class: formClass } }) }}
        {{ form_input(form.difficulty, 'difficulty') }}
        {{ form_input(form.ideas, 'ideas') }}
        
        {{ form_checkbox_group(form.satisfactionLevel, 'satisfactionLevel', {

            'fieldSetClass' : 'vertical',
            'legendClass' : 'form-label',
            'legend': 'satisfactionLevel',
            'items': [
                {'labelClass': 'block-label' },
                {'labelClass': 'block-label' },
                {'labelClass': 'block-label' },
                {'labelClass': 'block-label' },
                {'labelClass': 'block-label' },
            ]
        }) }}
        
        {{ form_checkbox_group(form.help, 'help', {

            'fieldSetClass' : 'vertical',
            'legendClass' : 'form-label',
            'legend': 'help',
            'items': [
                {'labelClass': 'block-label' },
                {'labelClass': 'block-label' },
                {'labelClass': 'block-label' },
                {'labelClass': 'block-label' },
            ]
        }) }}
        
        {% include 'AppBundle:Components/ButtonBar:_start.html.twig' %}
            {{ form_submit(form.save,'send') }}
        {% include 'AppBundle:Components/ButtonBar:_end.html.twig' %}
        

    {{ form_end(form) }}

{% endblock %}
