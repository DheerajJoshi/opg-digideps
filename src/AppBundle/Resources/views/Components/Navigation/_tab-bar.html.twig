{% if tabDataProvider is defined %}
    <div class="js-tabs nav-tabs behat-region-tab">
        <ul class="tabs-nav">
            {% for tab in tabDataProvider %}
                <li class="{{ tab.class }}">
                    <a id="{{ tab.tabId }}" href="{{ path(tab.href.path, tab.href.params) }}">{{ tab.label }}</a>
                    {% if tab.iconClass is defined %}
                        <span class="{{ tab.iconClass }}"></span>
                    {% endif %}
                </li>
            {% endfor %}
        </ul>
    </div>
{% endif %}
